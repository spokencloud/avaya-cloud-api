{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "format": "verbose"
  },
  "rules": {
    "eofline": false,
    "no-console": false,
    "no-debugger": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "interface-name": [true, "never-prefix"],
    "prefer-const": true
  }
}
