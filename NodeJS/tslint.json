{
    "extends": ["tslint:recommended"],
    "linterOptions": {
        "format": "verbose"
    },
    "rules": {
        "eofline": false,
        "no-console": false,
        "no-debugger": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "semicolon": [true, "never"],
        "prefer-const": true
    }
}
